<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>Website</display-name>
	<welcome-file-list>
		<welcome-file>Controller</welcome-file>
	</welcome-file-list>
	<context-param>
		<param-name>storage</param-name>
		<param-value>database</param-value>
	</context-param>
	<context-param>
		<param-name>db_url</param-name>
		<param-value>jdbc:postgresql://gegevensbanken.khleuven.be:51516/2TX32</param-value>
	</context-param>
	<context-param>
		<param-name>db_scheme_person</param-name>
		<param-value>r0620441_test.person</param-value>
	</context-param>
	<context-param>
		<param-name>db_scheme_product</param-name>
		<param-value>r0620441_test.product</param-value>
	</context-param>
	<context-param>
		<param-name>db_ssl</param-name>
		<param-value>true</param-value>
	</context-param>
	<context-param>
		<param-name>db_sslfactory</param-name>
		<param-value>org.postgresql.ssl.NonValidatingFactory</param-value>
	</context-param>
	<context-param>
		<param-name>db_user</param-name>
		<param-value>r0620441</param-value>
	</context-param>
	<context-param>
		<param-name>db_password</param-name>
		<param-value>PASSWORD</param-value>
	</context-param>
	<error-page>
		<location>/error.jsp</location>
	</error-page>
</web-app>